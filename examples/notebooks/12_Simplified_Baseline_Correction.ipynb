  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cr√©ation d'un spectre EPR 1D avec ligne de base polynomiale\n",
    "print(\"üî¨ G√©n√©ration d'un spectre EPR 1D avec ligne de base...\")\n",
    "\n",
    "# Axe du champ magn√©tique\n",
    "field = np.linspace(3300, 3400, 1000)\n",
    "\n",
    "# Ligne de base polynomiale (d√©rive + courbure)\n",
    "true_baseline = 20 + 0.1 * (field - 3350) + 0.0001 * (field - 3350)**2\n",
    "\n",
    "# Signal EPR (raie lorentzienne)\n",
    "center = 3350\n",
    "linewidth = 8\n",
    "amplitude = 50\n",
    "signal = amplitude * linewidth**2 / ((field - center)**2 + linewidth**2)\n",
    "\n",
    "# Donn√©es = signal + ligne de base + bruit\n",
    "noise = np.random.normal(0, 0.8, len(field))\n",
    "data_1d = signal + true_baseline + noise\n",
    "\n",
    "# Param√®tres comme retourn√©s par eprload\n",
    "params_1d = {\n",
    "    'XAXIS_NAME': 'Champ Magn√©tique',\n",
    "    'XAXIS_UNIT': 'G',\n",
    "    'MWFQ': 9.5e9\n",
    "}\n",
    "\n",
    "print(f\"‚úÖ Spectre g√©n√©r√©: {len(data_1d)} points\")\n",
    "print(f\"üìè Gamme: {field.min():.0f} - {field.max():.0f} G\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Correction avec la fonction simplifi√©e\n",
    "print(\"üîß Correction de ligne de base 1D...\")\n",
    "\n",
    "corrected_1d, baseline_1d = epyr.baseline.baseline_polynomial_1d_simple(\n",
    "    field, data_1d, params_1d, \n",
    "    order=2,                    # Polyn√¥me d'ordre 2\n",
    "    exclude_center=True,        # Exclure la r√©gion centrale (signal)\n",
    "    center_fraction=0.3         # 30% au centre\n",
    ")\n",
    "\n",
    "print(\"‚úÖ Correction termin√©e!\")\n",
    "\n",
    "# Visualisation\n",
    "fig, axes = plt.subplots(2, 1, figsize=(12, 10))\n",
    "\n",
    "# Graphique 1: Donn√©es et ligne de base\n",
    "axes[0].plot(field, data_1d, 'b-', alpha=0.7, label='Donn√©es avec ligne de base')\n",
    "axes[0].plot(field, baseline_1d, 'r-', linewidth=2, label='Ligne de base ajust√©e')\n",
    "axes[0].plot(field, true_baseline, 'g--', linewidth=2, alpha=0.8, label='Ligne de base vraie')\n",
    "axes[0].set_xlabel('Champ Magn√©tique (G)')\n",
    "axes[0].set_ylabel('Intensit√© (a.u.)')\n",
    "axes[0].set_title('Donn√©es 1D et ligne de base')\n",
    "axes[0].legend()\n",
    "axes[0].grid(True, alpha=0.3)\n",
    "\n",
    "# Graphique 2: Comparaison signal corrig√© vs vrai\n",
    "axes[1].plot(field, corrected_1d, 'b-', linewidth=1.5, label='Signal corrig√©')\n",
    "axes[1].plot(field, signal, 'r--', alpha=0.8, linewidth=2, label='Signal vrai')\n",
    "axes[1].set_xlabel('Champ Magn√©tique (G)')\n",
    "axes[1].set_ylabel('Intensit√© (a.u.)')\n",
    "axes[1].set_title('Signal corrig√© vs signal vrai')\n",
    "axes[1].legend()\n",
    "axes[1].grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Calcul de l'erreur\n",
    "rms_error = np.sqrt(np.mean((corrected_1d - signal)**2))\n",
    "print(f\"üìä Erreur RMS de la correction: {rms_error:.3f}\")"
