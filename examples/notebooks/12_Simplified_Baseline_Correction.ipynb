    "# Structure comme eprload\n",
    "x_data_2d = [field_2d, angle_2d]\n",
    "params_2d = {\n",
    "    'XAXIS_NAME': 'Champ Magn√©tique',\n",
    "    'YAXIS_NAME': 'Angle de Rotation',\n",
