    "# Cr√©ation de donn√©es 2D avec ligne de base\n",
    "print(\"üî¨ G√©n√©ration de donn√©es EPR 2D avec ligne de base...\")\n",
    "\n",
    "# Axes\n",
    "field_2d = np.linspace(3300, 3400, 120)\n",
    "angle_2d = np.linspace(0, 180, 60)\n",
    "F, A = np.meshgrid(field_2d, angle_2d)\n",
    "\n",
    "# Ligne de base 2D (surface polynomiale)\n",
    "baseline_2d = (15 + 0.05 * (F - 3350) + 0.02 * (A - 90) + \n",
    "               0.0001 * (F - 3350)**2 + 0.0002 * (A - 90)**2 + \n",
    "               0.00005 * (F - 3350) * (A - 90))\n",
    "\n",
    "# Signal EPR avec d√©pendance angulaire\n",
    "signal_2d = np.zeros_like(F)\n",
    "for i, angle in enumerate(angle_2d):\n",
    "    # Position de r√©sonance d√©pendante de l'angle\n",
    "    g_factor = 2.0 + 0.15 * np.cos(np.radians(angle))\n",
    "    res_field = 3350 + 20 * np.cos(np.radians(angle))\n",
    "    linewidth = 8 + 3 * np.sin(np.radians(angle))\n",
    "    \n",
    "    # Signal pour cet angle\n",
    "    amplitude = 40 + 20 * np.cos(np.radians(2 * angle))\n",
    "    signal_2d[i, :] = amplitude * linewidth**2 / ((field_2d - res_field)**2 + linewidth**2)\n",
