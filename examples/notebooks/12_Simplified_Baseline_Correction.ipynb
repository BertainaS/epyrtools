    "               0.00005 * (F - 3350) * (A - 90))\n",
    "\n",
    "# Signal EPR avec dÃ©pendance angulaire\n",
    "signal_2d = np.zeros_like(F)\n",
    "for i, angle in enumerate(angle_2d):\n",
    "    # Position de rÃ©sonance dÃ©pendante de l'angle\n",
