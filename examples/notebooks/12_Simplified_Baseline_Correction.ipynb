    "\n",
    "# DonnÃ©es = signal + ligne de base + bruit\n",
    "noise = np.random.normal(0, 0.8, len(field))\n",
    "data_1d = signal + true_baseline + noise\n",
    "\n",
    "# ParamÃ¨tres comme retournÃ©s par eprload\n",
    "params_1d = {\n",
    "    'XAXIS_NAME': 'Champ MagnÃ©tique',\n",
    "    'XAXIS_UNIT': 'G',\n",
    "    'MWFQ': 9.5e9\n",
    "}\n",
    "\n",
    "print(f\"âœ… Spectre gÃ©nÃ©rÃ©: {len(data_1d)} points\")\n",
