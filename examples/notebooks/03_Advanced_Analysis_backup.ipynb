    "print()\n",
    "\n",
    "g_factors = []\n",
    "if zero_crossings:\n",
    "    print(\"Peak positions and g-factors:\")\n",
    "    print(f\"{'Peak':<8} {'Field (G)':<12} {'g-factor':<12} {'Î”g':<12}\")\n",
    "    print(\"-\" * 48)\n",
    "    \n",
    "    for i, field in enumerate(zero_crossings):\n",
    "        g = calculate_g_factor(field, freq_hz)\n",
